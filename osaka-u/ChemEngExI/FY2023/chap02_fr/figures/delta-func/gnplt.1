set terminal tikz
set output "graph.tex"
#
# ... files and functions ...
# ... key settings ...
set key spacing 1.5
t1='$M=1$'
# ... range settings ...
set xrange [-10:10]
set yrange [-0.5:2.2]
# ... axis label settings ...
#set xlabel "$x$" 
#set xtics 1 
#set ytics 1 
# ... ratio and size settings ...
set size 0.5, 0.5
# ... frame settings ...
#set border back
#set grid lw 2
#set border 15 lw 3

set dashtype 1 (3,2)

set arrow 1 from 0, -0.5 to 0, 2.2 nohead lw 2 lc rgb "black"
set arrow 2 from -10, 0 to 10, 0 nohead lw 2 lc rgb "black"

set noxtics
set noytics

set noborder
# ... plot part ...
plot 3.0*exp(-5.0*x**2)  w l lw 4 lc rgb "black" notitle
